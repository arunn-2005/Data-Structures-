#include<iostream> 
using namespace std; 
int main() 
{ 
    int arr[]={3,5,1,7,4,9,12}; 
    int ct=0; 
    int targ=9; 
    int n=7; 
    for(int i=0;i<n;++i) 
    { 
        for( int j=i+1;j<n;++j){ 
            for(int k=j+1;k<n;++k) 
            { 
                if(arr[i]+arr[j]+arr[k]==targ){ 
                    ct++; 
                }}}} 
    cout<<ct;} 
